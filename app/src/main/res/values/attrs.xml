<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToolBar">
        <attr name="right_img" format="reference" />
        <attr name="right_text" format="string" />
        <attr name="title_text" format="string" />
        <attr name="title_search" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AddSubView">
        <attr name="value" format="integer|reference" />
        <attr name="min_value" format="integer|reference" />
        <attr name="max_value" format="integer|reference" />

        <attr name="value_background" format="reference" />
        <attr name="btn_add_background" format="reference" />
        <attr name="btn_sub_background" format="reference" />
    </declare-styleable>

</resources>